
{
  "manifest_version": 2,

  "name": "Price Checker",
  "version": "1.0",
  "browser_action": {
    "default_popup": "window.html"
  },
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
	"matches": ["https://www.google.com/*"],
	"js": ["jquery-1.11.0.js", "content.js"],
  "css" : ["stylesheet.css"],
	"run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "logo.png"
  ],
  "permissions": [
    "tabs",
    "notifications",
    "http://*/",
    "https://*/"
  ]
  
}
